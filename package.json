{
  "name": "electric-scan",
  "version": "0.2.0",
  "description": "Electric Scan",
  "license": "GPLv2",
  "homepage": "https://bishopfox.com/",
  "author": {
    "name": "moloch",
    "email": "contact@bishopfox.com"
  },
  "repository": "https://github.com/moloch--/electric-scan",
  "main": "main.js",
  "private": true,
  "scripts": {
    "build": "npm run electron:main-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "electron:main-tsc": "tsc -p tsconfig.main.json",
    "electron:local": "npm run build:dev && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --config ./builder/linux.json --linux --publish=never",
    "electron:windows": "npm run build:prod && electron-builder build --config ./builder/windows-exe.json --windows --publish=never",
    "electron:mac-x86": "npm run build:prod && electron-builder build --config ./builder/macos-x64.json --mac --publish=never",
    "electron:mac-arm64": "npm run build:prod && electron-builder build --config ./builder/macos-arm64.json --mac --publish=never",
    "publish:mac": "npm run build:prod && electron-builder build --config ./builder/macos-x64.json --mac --publish=always && electron-builder build --config ./builder/macos-arm64.json --mac --publish=always",
    "publish:windows": "npm run build:prod && electron-builder build --config ./builder/windows-exe.json --windows --publish=always && electron-builder build --config ./builder/windows-nsis.json --windows --publish=always",
    "publish:linux": "npm run build:prod && electron-builder build --config ./builder/linux.json --linux --publish=always",
    "test": "ng test"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.13",
    "@angular-extensions/lint-rules": "^1.1.1",
    "@angular/cli": "12.0.0",
    "@angular/common": "12.0.0",
    "@angular/compiler": "12.0.0",
    "@angular/compiler-cli": "12.0.0",
    "@angular/core": "12.0.0",
    "@angular/forms": "12.0.0",
    "@angular/language-service": "12.0.0",
    "@angular/platform-browser": "12.0.0",
    "@angular/platform-browser-dynamic": "12.0.0",
    "@angular/router": "12.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "5.2.7",
    "@types/node": "^12.12.24",
    "@types/uuid": "^3.4.5",
    "@types/webgl2": "0.0.5",
    "chai": "4.2.0",
    "core-js": "3.3.2",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "electron-reload": "1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ts-node": "8.4.1",
    "tslint": "~6.1.0",
    "typescript": "^4.2.4",
    "zone.js": "~0.11.4"
  },
  "engines": {
    "node": "^12.14.1 || ^14.0.0"
  },
  "dependencies": {
    "@angular/animations": "^12.0.0",
    "@angular/cdk": "^12.0.0",
    "@angular/localize": "^12.0.0",
    "@angular/material": "^12.0.0",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@tensorflow/tfjs": "^3.6.0",
    "ajv": "^6.10.2",
    "base64-arraybuffer": "^0.2.0",
    "electron-context-menu": "^3.1.0",
    "ip-cidr": "^2.0.6",
    "moment": "^2.24.0",
    "ngx-infinite-scroll": "^8.0.1",
    "pako": "^1.0.10",
    "pixelmatch": "^5.1.0",
    "rxjs": "^6.5.3",
    "uuid": "^3.3.3",
    "write-file-atomic": "^3.0.1"
  }
}
